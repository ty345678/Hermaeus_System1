<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/rootLayout"
    >


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:background="@color/teal_700"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme" />


    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_LeftMirror_Driving"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV01"
        app:layout_constraintEnd_toStartOf="@id/guidelineV33"

        />


    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_Back_Driving"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV33"
        app:layout_constraintEnd_toStartOf="@id/guidelineV66"

        />

    <com.longdo.mjpegviewer.MjpegView
    android:id="@+id/stream_RightMirror_Driving"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintBottom_toTopOf="@id/guidelineH99"
    app:layout_constraintTop_toBottomOf="@id/guidelineH01"
    app:layout_constraintStart_toEndOf="@id/guidelineV66"
    app:layout_constraintEnd_toStartOf="@id/guidelineV99"

    />







    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_RightMirror_RightTurn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV33"
        app:layout_constraintEnd_toStartOf="@id/guidelineV99"

        />

    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_Back_RightTurn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV01"
        app:layout_constraintEnd_toStartOf="@id/guidelineV33"

        />








    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_LeftMirror_LeftTurn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV01"
        app:layout_constraintEnd_toStartOf="@id/guidelineV66"

        />

    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_Back_LeftTurn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV66"
        app:layout_constraintEnd_toStartOf="@id/guidelineV99"

        />









    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_LeftMirror_Reverse"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV01"
        app:layout_constraintEnd_toStartOf="@id/guidelineV24"

        />

    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_Back_Reverse"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV24"
        app:layout_constraintEnd_toStartOf="@id/guidelineV75"

        />
    <com.longdo.mjpegviewer.MjpegView
        android:id="@+id/stream_RightMirror_Reverse"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintTop_toBottomOf="@id/guidelineH01"
        app:layout_constraintStart_toEndOf="@id/guidelineV75"
        app:layout_constraintEnd_toStartOf="@id/guidelineV99"

        />













    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Reverse"
        android:onClick="Reverse"

        app:layout_constraintTop_toBottomOf="@id/guidelineH83"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintStart_toEndOf="@id/guidelineV16"
        app:layout_constraintEnd_toStartOf="@id/guidelineV33"
        />
    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Left Turn"
        android:onClick="LeftTurn"

        app:layout_constraintTop_toBottomOf="@id/guidelineH83"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintStart_toEndOf="@id/guidelineV33"
        app:layout_constraintEnd_toStartOf="@id/guidelineV50"
        />
    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Right Turn"
        android:onClick="RightTurn"

        app:layout_constraintTop_toBottomOf="@id/guidelineH83"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintStart_toEndOf="@id/guidelineV50"
        app:layout_constraintEnd_toStartOf="@id/guidelineV66"

        />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Driving"
        android:onClick="Driving"

        app:layout_constraintTop_toBottomOf="@id/guidelineH83"
        app:layout_constraintBottom_toTopOf="@id/guidelineH99"
        app:layout_constraintStart_toEndOf="@id/guidelineV66"
        app:layout_constraintEnd_toStartOf="@id/guidelineV83"

        />
    <androidx.constraintlayout.widget.Guideline
        android:saveEnabled="true"
        android:id="@+id/guidelineV01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.001" />
    <androidx.constraintlayout.widget.Guideline
        android:saveEnabled="true"
        android:id="@+id/guidelineV16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.16" />
    <androidx.constraintlayout.widget.Guideline
        android:saveEnabled="true"
        android:id="@+id/guidelineV24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.24" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineV33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:saveEnabled="true"
        app:layout_constraintGuide_percent=".33" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineV50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.50" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineV66"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.66" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineV75"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.75" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineV83"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.83" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineV99"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.999" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineH01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.11"
        />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineH33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.40" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineH66"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.70" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineH83"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.85" />
    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineH99"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent=".999" />

</androidx.constraintlayout.widget.ConstraintLayout>